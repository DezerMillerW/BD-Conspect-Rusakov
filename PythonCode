Задание 18
a=int(input("Введите трехзначное число:"))
b = a//100
c = (a-b*100)//10
v = (a-b*100)-c*10
print(b+c+v)
print(b*c*v)

Задание 19
a=int(input("Введите трехзначное число:"))
b = a//100
c = (a-b*100)//10
v = (a-b*100)-c*10
b = str(b)
c = str(c)
v = str(v)
print(b+c+v)
print(b+v+c)
print(c+b+v)
print(c+v+b)
print(v+b+c)
print(v+c+b)
