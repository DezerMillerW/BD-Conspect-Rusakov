16.
a = input()
b = input()
c = input()
if len(a)>len(b)>len(c) or len(c)>len(b)>len(a):
    print(a)
    print(c)
elif len(a)>len(c)>len(b) or len(b)>len(c)>len(a):
    print(a)
    print(b)
else:
    print(b)
    print(c)

