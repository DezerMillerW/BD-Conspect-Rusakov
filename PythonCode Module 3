25(другое решение. В первом перепутал знак со знаком модуля в условии)
a = float(input())
b = 0
if a%1!=0:
    b = a//1
    a = a//1+1
else:
    b = a
print(a+b)

26.
a = float(input())
b = float(input())
c = float(input())
d = ((b**2)-(4*a*c))**0.5
if d > 0:
    x1 = ((b*-1)-d)/(2*a)
    x2 = ((b*-1)+d)/(2*a)
    if x1>x2:
        print(x2,x1)
    else:
        print(x1,x2)
elif d == 0:
    x1 = (b*-1)/(2*a)
    print(x1)
else:
    print('корней нет')

27.
import math
a = float(input())
b = float(input())
s = (a*(b**2)/(4*math.tan(math.pi/a)))
print(s)
