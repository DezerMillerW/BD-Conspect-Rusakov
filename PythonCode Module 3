8.
list1 = list()
min = 999999999
max = 0
for i in range(5):
    a = int(input())
    list1.append(a)
for i in list1:
    if min>i:
        min = i
    if max<i:
        max = i
print('max = ', max)
print('min = ', min)

9.
a = int(input())
b = int(input())
c = int(input())
if a > b >c:
    print(a)
    print(b)
    print(c)
elif a > c >b:
    print(a)
    print(c)
    print(b)
elif b>a>c:
    print(b)
    print(a)
    print(c)
elif b>c>a:
    print(b)
    print(c)
    print(a)
elif c>a>b:
    print(c)
    print(a)
    print(b)
elif c>b>a:
    print(c)
    print(b)
    print(a)

10.
list1=list(input())
list2= list()
for i in list1:
    i = int(i)
    list2.append(i)
if list2[0]-list2[2] == list2[1] or list2[2]-list2[0] == list2[1]:
    print('yes')
else:
    print('no')

11.
a = 0
w = 0
b = list()
while a!=5:
    c = float(input())
    b.append(c)
    a+=1
for i in b:
    if i < 0:
        i = i*(-1)
    w+=i
print(w)

12.
p1 = int(input())
p2 = int(input())
q1= int(input())
q2= int(input())
w = p1-p2
d = q1-q2
if w < 0:
    w = w*-1
if d < 0:
    d = d*-1
print(w+d)

